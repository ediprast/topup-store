<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No.</th>
      <th>Name</th>
      <th>Bank Name</th>
      <th>Number</th>
      <th>Action</th>
    </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < bank.length; i++) { %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= bank[i].name %></td>
          <td><%= bank[i].bankName %></td>
          <td><%= bank[i].noRekening %></td>
          <td class="d-flex">
            <a href="/bank/edit/<%= bank[i]._id %>" class="btn btn-sm btn-info mr-2">
              <i class="fas fa-pencil-alt"></i> Edit</a>
              <form action="/bank/delete/<%= bank[i]._id %>?_method=DELETE" method="POST">
            <button class="btn btn-sm btn-danger">
              <i class="fas fa-trash"></i> Delete</button>
            </form>
          </td>
        </tr>
        <% } %>
    </tbody>
</table>